datasource db {
  provider = "sqlite"
  url      = "file:./pokemon.db"
}

generator client {
  provider = "prisma-client-js"
}

model TeamPokemon {
  id       Int      @id @default(autoincrement())
  uuid     String
  name     String
  level    Int
  xp       Int
  hp       Int
  maxHp    Int
  attack   Int
  defense  Int
  speed    Int
  createdAt DateTime @default(now())
}
